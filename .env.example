# Vault Configuration
VAULT_TOKEN=your-vault-service-token-here

# Vault Unseal Keys (REQUIRED - получите их при инициализации Vault)
# Нужно минимум 3 из 5 ключей для unsealing
VAULT_UNSEAL_KEY_1=your-unseal-key-1-here
VAULT_UNSEAL_KEY_2=your-unseal-key-2-here
VAULT_UNSEAL_KEY_3=your-unseal-key-3-here
VAULT_UNSEAL_KEY_4=your-unseal-key-4-here
VAULT_UNSEAL_KEY_5=your-unseal-key-5-here

# Vault Unsealer Configuration (опционально)
UNSEALER_MAX_RETRIES=50
UNSEALER_RETRY_DELAY=5
UNSEALER_MONITOR_INTERVAL=30
UNSEALER_LOG_LEVEL=INFO

# JWT Configuration
JWT_SECRET_KEY=super-secret-jwt-key-for-content-factory-2024

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO

# ВАЖНО: Скопируйте этот файл в .env и замените значения на реальные!
# НИКОГДА не коммитьте .env файл с реальными ключами в Git! 